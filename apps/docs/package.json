{
  "name": "docs",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vitepress dev --port 8000",
    "build-only": "cross-env NODE_OPTIONS=--max-old-space-size=4096 vitepress build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json",
    "build": "bun run type-check && bun run build-only",
    "preview": "vitepress preview",
    "lint": "eslint --fix src tests",
    "test:lint": "eslint src tests",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "test": "bun run test:lint && bun run test:unit:ci"
  },
  "devDependencies": {
    "@iconify-json/bi": "^1.2.7",
    "@iconify-json/logos": "^1.2.10",
    "@stackblitz/sdk": "^1.11.0",
    "@toycode/markdown-it-class": "^1.2.4",
    "@tsconfig/node24": "^24.0.4",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^24.10.9",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "@vueuse/core": "^14.1.0",
    "bootstrap": "^5.3.8",
    "bootstrap-vue-next": "workspace:^",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-vue": "^10.7.0",
    "happy-dom": "^20.4.0",
    "jiti": "^2.6.1",
    "markdown-it": "^14.1.0",
    "npm-run-all2": "^8.0.4",
    "oxlint": "^1.42.0",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "unplugin-icons": "^23.0.1",
    "unplugin-vue-components": "^31.0.0",
    "vite": "^7.3.1",
    "vitepress": "1.6.3",
    "vitest": "^4.0.18",
    "vue": "^3.5.27",
    "vue-component-type-helpers": "^3.2.4",
    "vue-tsc": "^3.2.4",
    "yaml": "^2.8.2"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache --fix",
    "*": "prettier --write --ignore-unknown"
  }
}
