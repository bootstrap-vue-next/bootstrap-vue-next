{
  "name": "docs",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vitepress dev --port 8000",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 vitepress build",
    "preview": "vitepress preview",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path ../../.gitignore --fix src",
    "test:lint": "eslint --ext .js,.ts,.vue --ignore-path ../../.gitignore src",
    "test": "pnpm run test:lint"
  },
  "devDependencies": {
    "@iconify-json/bi": "^1.1.17",
    "@iconify-json/simple-icons": "^1.1.62",
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node20": "^20.1.0",
    "@types/node": "^20.4.5",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "@vueuse/core": "^10.2.1",
    "bootstrap": "^5.3.1",
    "bootstrap-vue-next": "workspace:^",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "eslint-define-config": "^1.21.0",
    "eslint-plugin-vue": "^9.15.1",
    "prettier": "^3.0.0",
    "sitemap": "^7.1.1",
    "typescript": "^5.1.6",
    "unplugin-icons": "^0.16.5",
    "vitepress": "1.0.0-beta.6",
    "vue": "^3.3.4"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache --fix",
    "*": "prettier --write --ignore-unknown"
  }
}
