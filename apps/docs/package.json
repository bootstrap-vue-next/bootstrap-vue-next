{
  "name": "docs",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bunx vitepress dev --port 8000",
    "build-only": "bunx cross-env NODE_OPTIONS=--max-old-space-size=4096 vitepress build",
    "type-check": "bunx vue-tsc --noEmit -p tsconfig.app.json",
    "build": "bun run type-check && bun run build-only",
    "preview": "bunx vitepress preview",
    "lint": "bunx eslint --fix src tests",
    "test:lint": "bunx eslint src tests",
    "test:unit": "bunx vitest",
    "test:unit:ci": "bunx vitest run",
    "test": "bun run test:lint && bun run test:unit:ci"
  },
  "devDependencies": {
    "@iconify-json/bi": "^1.2.7",
    "@iconify-json/logos": "^1.2.10",
    "@rushstack/eslint-patch": "^1.15.0",
    "@stackblitz/sdk": "^1.11.0",
    "@toycode/markdown-it-class": "^1.2.4",
    "@tsconfig/node22": "^22.0.5",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.19.7",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "@vueuse/core": "^13.9.0",
    "bootstrap": "^5.3.8",
    "bootstrap-vue-next": "workspace:^",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.2",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-vue": "^10.7.0",
    "happy-dom": "^20.3.4",
    "markdown-it": "^14.1.0",
    "prettier": "^3.8.0",
    "typescript": "^5.9.3",
    "unplugin-icons": "^22.5.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^6.4.1",
    "vitepress": "1.6.3",
    "vitest": "^3.2.4",
    "vue": "^3.5.27",
    "vue-component-type-helpers": "^2.1.10",
    "vue-tsc": "^3.2.2",
    "yaml": "^2.7.1"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache --fix",
    "*": "prettier --write --ignore-unknown"
  }
}
