{
  "name": "docs",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vitepress dev --port 8000",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 vitepress build",
    "preview": "vitepress preview",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path ../../.gitignore --fix src",
    "test:lint": "eslint --ext .js,.ts,.vue --ignore-path ../../.gitignore src",
    "test": "pnpm run test:lint"
  },
  "devDependencies": {
    "@iconify-json/bi": "^1.2.0",
    "@iconify-json/simple-icons": "^1.2.2",
    "@rushstack/eslint-patch": "^1.10.4",
    "@toycode/markdown-it-class": "^1.2.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^22.5.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "@vueuse/core": "^11.0.3",
    "bootstrap": "^5.3.3",
    "bootstrap-vue-next": "workspace:^",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-vue": "^9.28.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "unplugin-icons": "^0.19.3",
    "vitepress": "1.3.4",
    "vue": "^3.5.3"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache --fix",
    "*": "prettier --write --ignore-unknown"
  }
}
